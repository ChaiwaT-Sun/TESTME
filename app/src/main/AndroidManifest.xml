<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learn.testme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_testme_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_testme_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInActivity" />
        <activity android:name=".SignUpStudent" />
        <activity android:name=".Home" />
        <activity
            android:name=".Start"
            android:label="@string/title_activity_start"
            android:theme="@style/AppTheme" />
        <activity android:name=".Play" />
        <activity android:name=".Done"></activity>
    </application>

</manifest>